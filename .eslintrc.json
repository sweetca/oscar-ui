{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
      "react/prop-types": 0
    }
}